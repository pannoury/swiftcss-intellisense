{
  "name": "swiftcss",
  "displayName": "SwiftCSS",
  "description": "Intellisense for SwiftCSS",
  "publisher": "Patrick Tannoury",
  "icon": "logo_yellow.png",
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.82.0"
  },
  "capabilities": {
    "colorProvider": "true",
    "hoverProvider": "true",
    "completionProvider": {
      "resolveProvider": "true"
    }
  },
  "activationEvents": [
    "onCommand:extension.activate"
  ],
  "categories": [
    "Other"
  ],
  "main": "./out/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "swiftcss.helloWorld",
        "title": "SwiftCSS: Start Intellisense"
      }
    ],
    "languages": [
      {
        "id": "html",
        "aliases": [
          "HTML",
          "html"
        ],
        "extensions": [
          ".html",
          ".htm",
          ".htmx",
          ".htx"
        ]
      },
      {
        "id": "javascript",
        "aliases": ["JavaScript", "js"],
        "extensions": [".js"]
      },
      {
        "id": "javascriptreact",
        "aliases": ["JavaScript React", "jsx"],
        "extensions": [".jsx"]
      },
      {
        "id": "typescript",
        "aliases": ["TypeScript", "ts"],
        "extensions": [".ts"]
      },
      {
        "id": "typescriptreact",
        "aliases": ["TypeScript React", "tsx"],
        "extensions": [".tsx"]
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "pretest": "npm run compile && npm run lint",
    "lint": "eslint src --ext ts",
    "test": "node ./out/test/runTest.js"
  },
  "devDependencies": {
    "@types/mocha": "^10.0.1",
    "@types/node": "16.x",
    "@types/vscode": "^1.82.0",
    "@typescript-eslint/eslint-plugin": "^6.4.1",
    "@typescript-eslint/parser": "^6.4.1",
    "@vscode/test-electron": "^2.3.4",
    "eslint": "^8.47.0",
    "glob": "^10.3.3",
    "mocha": "^10.2.0",
    "typescript": "^5.1.6"
  },
  "dependencies": {
    "culori": "^3.2.0"
  }
}
